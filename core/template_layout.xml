<?xml version="1.0" encoding="iso-8859-1"?>

<root>
    <template name="TypeLayout">
        <genericcontrol name="TypeLayout" >
            <_class>LayoutBeginHorizontal</_class>
            <anchored><top/><left/></anchored><invisible/>
            <script file="core/scripts/layout.lua" />
        </genericcontrol>
    </template>

    <template name="LayoutEnd">
        <genericcontrol name="LayoutEnd" >
            <_class>LayoutEnd</_class>
            <anchored><top/><left/></anchored><invisible/>
        </genericcontrol>
    </template>

    <template name="LayoutBeginHorizontal">
        <genericcontrol name="LayoutBeginHorizontal" >
            <_class>LayoutBeginHorizontal</_class>
            <anchored><top/><left/></anchored><invisible/>
        </genericcontrol>
    </template>

    <template name="LayoutBeginForm">
        <genericcontrol name="LayoutBeginForm" >
            <_class>LayoutBeginForm</_class>
            <anchored><top/><left/></anchored><invisible/>
        </genericcontrol>
    </template>

    <template name="layout_genericcontrol">
        <genericcontrol>
            <_class>genericcontrol</_class>
            <anchored><top/><left/></anchored>
        </genericcontrol>
    </template>

    <template name="layout_stringcontrol">
        <stringcontrol>
            <_class>stringcontrol</_class>
            <anchored><top/><left/></anchored>
        </stringcontrol>
    </template>

    <template name="layout_stringfield">
        <stringfield>
            <_class>stringfield</_class>
            <anchored><top/><left/></anchored>
        </stringfield>
    </template>

    <template name="layout_numberfield">
        <numberfield>
            <_class>numberfield</_class>
            <anchored><top/><left/></anchored>
        </numberfield>
    </template>

    <template name="layout_buttoncontrol">
        <buttoncontrol>
            <_class>buttoncontrol</_class>
            <anchored><top/><left/></anchored>
        </buttoncontrol>
    </template>

</root>