<?xml version="1.0" encoding="iso-8859-1"?>

<root>
    <template name="TypeLayout">
        <genericcontrol name="TypeLayout" >
            <anchored>
                <top/>
                <left/>
            </anchored>
            <script file="core/scripts/layout.lua" />
            <invisible/>
        </genericcontrol>
    </template>

    <template name="LayoutEnd">
        <stringcontrol name="LayoutEnd" >
            <_class>LayoutEnd</_class>
            <anchored>
                <top/>
                <left/>
            </anchored>
            <static text="LayoutEnd"/>
            <invisible/>
        </stringcontrol>
    </template>

    <template name="LayoutBeginHorizontal">
        <stringcontrol name="LayoutBeginHorizontal" >
            <_class>LayoutBeginHorizontal</_class>
            <anchored>
                <top/>
                <left/>
            </anchored>
            <static text="LayoutBeginHorizontal"/>
            <invisible/>
        </stringcontrol>
    </template>

    <template name="LayoutBeginForm">
        <stringcontrol name="LayoutBeginForm" >
            <_class>LayoutBeginForm</_class>
            <anchored>
                <top/>
                <left/>
            </anchored>
            <static text="LayoutBeginForm"/>
            <invisible/>
        </stringcontrol>
    </template>

    <template name="layout_genericcontrol">
        <genericcontrol>
            <anchored>
                <top/>
                <left/>
            </anchored>
        </genericcontrol>
    </template>

    <template name="layout_stringcontrol">
        <stringcontrol>
            <anchored>
                <top/>
                <left/>
            </anchored>
        </stringcontrol>
    </template>

    <template name="layout_stringfield">
        <stringfield>
            <anchored>
                <top/>
                <left/>
            </anchored>
        </stringfield>
    </template>

    <template name="layout_numberfield">
        <numberfield>
            <anchored>
                <top/>
                <left/>
            </anchored>
        </numberfield>
    </template>

    <template name="layout_buttoncontrol">
        <buttoncontrol>
            <anchored>
                <top/>
                <left/>
            </anchored>
        </buttoncontrol>
    </template>

</root>