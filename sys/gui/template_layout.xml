    <template name="Layout">
        <genericcontrol name="_main" >
            <_class>Layout</_class>
            <anchored><top/><left/></anchored>
            <invisible/>
            <disabled/>
        </genericcontrol>
    </template>

    <template name="LayoutForm">
        <Layout>
            <type>form</type>
        </Layout>
    </template>

    <template name="LayoutHorizontal">
        <Layout>
            <type>horizontal</type>
        </Layout>
    </template>

    <template name="layout_genericcontrol">
        <genericcontrol>
            <_class>genericcontrol</_class>
            <layout>_main</layout>
            <anchored><top/><left/></anchored>
        </genericcontrol>
    </template>

    <template name="layout_stringcontrol">
        <stringcontrol>
            <_class>stringcontrol</_class>
            <layout>_main</layout>
            <anchored><top/><left/></anchored>
        </stringcontrol>
    </template>

    <template name="layout_stringfield">
        <stringfield>
            <_class>stringfield</_class>
            <layout>_main</layout>
            <anchored><top/><left/></anchored>
        </stringfield>
    </template>

    <template name="layout_numberfield">
        <numberfield>
            <_class>numberfield</_class>
            <layout>_main</layout>
            <anchored><top/><left/></anchored>
        </numberfield>
    </template>

    <template name="layout_buttoncontrol">
        <buttoncontrol>
            <_class>buttoncontrol</_class>
            <layout>_main</layout>
            <anchored><top/><left/></anchored>
        </buttoncontrol>
    </template>

</root>